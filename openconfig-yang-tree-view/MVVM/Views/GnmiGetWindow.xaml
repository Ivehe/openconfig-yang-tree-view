<Window x:Class="openconfig_yang_tree_view.MVVM.Views.GnmiGetWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:openconfig_yang_tree_view.MVVM.Views"
        mc:Ignorable="d" Background="#002032"
        Title="GnmiGetWindow" Height="450" Width="800">
    <Grid>
        <TextBox Name="txtResponse" Style="{StaticResource GetResponseTextboxTheme}" Width="600" Height="380" HorizontalAlignment="Left" Margin="20, 0, 0, 0"/>
        <Button Content="Copy" Click="Button_Click" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,27,114,0" Height="auto" Style="{StaticResource SelectButtonTheme}" />
    </Grid>
</Window>
